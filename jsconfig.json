{
	"typeAcquisition": {
		"include": ["jest"]
	},
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@pages/*": ["/pages/*"],
			"@components/*": ["/components/*"],
			"@styles/*": ["/styles/*"],
			"@store/*": ["/store/*"],
			"@hooks/*": ["/hooks/*"],
			"@utils/*": ["/utils/*"]
		}
	},
	"exclude": ["node_modules"]
}
